{
  "expo": {
    "name": "expo-boilerplate",
    "slug": "expo-boilerplate",
    "sdkVersion": "36.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.author.mobile",
      "buildNumber": "20",
      "infoPlist": {
        "NSCameraUsageDescription": "Camera is needed to scan Kegs ID.",
        "NFCReaderUsageDescription": "NFC is required to scan device unique IDs.",
        "NSBluetoothAlwaysUsageDescription": "Bluetooth access is required to scan for device beacons.",
        "NSBluetoothPeripheralUsageDescription": "Bluetooth access is required to scan for device beacons."
      },
      "config": {
        "usesNonExemptEncryption": false
      },
      "publishBundlePath": "ios/expo-boilerplate/Supporting/shell-app.bundle",
      "publishManifestPath": "ios/expo-boilerplate/Supporting/shell-app-manifest.json"
    },
    "android": {
      "package": "com.author.mobile",
      "versionCode": 20,
      "publishBundlePath": "android/app/src/main/assets/shell-app.bundle",
      "publishManifestPath": "android/app/src/main/assets/shell-app-manifest.json"
    },
    "isDetached": true,
    "detach": {
      "iosExpoViewUrl": "https://s3.amazonaws.com/exp-exponent-view-code/ios-v2.14.6-sdk36.0.0-8ea388d1-463b-4e91-bbf7-2d95d0174c96.tar.gz",
      "androidExpoViewUrl": "https://s3.amazonaws.com/exp-exponent-view-code/android-v2.14.2-sdk36.0.0-ce7e16db-fd5d-4fdd-bdeb-701ba94a04d1.tar.gz"
    }
  }
}