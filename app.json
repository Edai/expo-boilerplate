{
  "expo": {
    "name": "expo-boilerplate",
    "slug": "expo-boilerplate",
    "platforms": ["ios", "android"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.author.mobile",
      "buildNumber": "20",
      "infoPlist": {
        "NSCameraUsageDescription": "Camera is needed to scan.",
        "NFCReaderUsageDescription": "NFC is required to scan unique IDs.",
        "NSBluetoothAlwaysUsageDescription": "Bluetooth access is required to scan for device.",
        "NSBluetoothPeripheralUsageDescription": "Bluetooth access is required to scan for device."
      },
      "config": {
        "usesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "com.author.mobile",
      "versionCode": 20
    }
  }
}
